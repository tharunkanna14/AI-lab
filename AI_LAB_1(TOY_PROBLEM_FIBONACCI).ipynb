        "def fib  (n):\r\n",
        "     if n ==1 or n ==2:\r\n",
        "         return 1\r\n",
        "     return fib (n-1) + fib (n-2)\r\n",
        "print(\"Enter the number of series you want to print: \")\r\n",
        "a=int(input())\r\n",
        "print(\"The following are list of \",a,\"fibonacci series:\")\r\n",
        "for n in range(1, a+1):\r\n",
        "  print (fib(n))\r\n",
        "\r\n",
        "\r\n",
        "\r\n"
      ],
